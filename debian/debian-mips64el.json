{
    "variables": {
        "vm_name": "debian-mips64el",
        "iso_url": "https://cdimage.debian.org/debian-cd/current/mips64el/iso-cd/debian-9.9.0-mips64el-netinst.iso",
        "iso_checksum_type": "sha512",
        "iso_checksum_url": "https://cdimage.debian.org/debian-cd/current/mips64el/iso-cd/SHA512SUMS",
        "ssh_username": "vagrant",
        "ssh_password": "vagrant",
        "ssh_wait_timeout": "6h",
        "disk_size_MB": "8000",
        "ram_MiB": "1024",
        "boot_wait": "1m",
        "http_directory": "http",
        "shutdown_command": "echo 'vagrant' | sudo -S halt -p"
    },
    "provisioners": [
        {
            "type": "shell",
            "execute_command": "echo 'vagrant' | {{ .Vars }} sudo -ES sh {{.Path}}",
            "scripts": [
                "fix-debconf.debian.sh",
                "fix-virtual-drivers.gnulinux.sh",
                "fix-vagrant-ssh.debian.sh",
                "cleanup.debian.sh"
            ]
        }
    ],
    "post-processors": [
        {
            "type": "vagrant",
            "output": "{{ user `vm_name` }}-{{.Provider}}.box",
            "compression_level": "9",
            "vagrantfile_template": "mips64el.Vagrantfile"
        }
    ],
    "builders": [
        {
            "type": "qemu",
            "accelerator": "tcg",
            "machine_type": "malta",
            "use_default_display": true,
            "vm_name": "{{ user `vm_name` }}",
            "iso_url": "{{ user `iso_url` }}",
            "iso_checksum_type": "{{ user `iso_checksum_type` }}",
            "iso_checksum_url": "{{ user `iso_checksum_url` }}",
            "ssh_username": "{{ user `ssh_username` }}",
            "ssh_password": "{{ user `ssh_password` }}",
            "ssh_wait_timeout": "{{ user `ssh_wait_timeout` }}",
            "disk_size": "{{ user `disk_size_MB` }}",
            "qemu_binary": "qemu-system-mips64el",
            "qemuargs": [
                ["-cpu", "5KEc"],
                ["-m", "{{ user `ram_MiB` }}"],
                ["-initrd", "initrd.gz"],
                ["-kernel", "vmlinux-4.9.0-9-5kc-malta"],
                ["-append", "nokaslr console=ttyS0 root=/dev/vda1"]
            ],
            "http_directory": "{{ user `http_directory` }}",
            "boot_wait": "{{ user `boot_wait` }}",
            "boot_command": [




                "<wait20m>",




                "<leftCtrlOn><leftAltOn>3<leftAltOff><leftCtrlOff>",
                "<enter><wait>",
                "<enter><wait2m>",
                "<end><bs><bs><bs><bs><bs><bs>vagrant<enter><wait>",
                "vagrantup.com<enter><wait10s>",
                "<enter><wait>",
                "<enter><wait>",
                "<enter><wait5m>",
                "vagrant<enter><wait>",
                "vagrant<enter><wait>",
                "Vagrant<enter><wait>",
                "<enter><wait>",
                "vagrant<enter><wait>",
                "vagrant<enter><wait10s>",
                "<enter><wait2m>",
                "<enter><wait10s>",
                "<enter><wait1m>",
                "<enter><wait1m>",
                "<enter><wait10s>",
                "<left><enter><wait1h>",
                "<left><enter><wait5m>",
                "<down><down><down><down><down><down><spacebar><wait>",
                "<tab><wait>",
                "<enter><wait2h>",
                "<enter><wait5m>",
                "<leftCtrlOn><leftAltOn>2<leftAltOff><leftCtrlOff>",
                "eject -f ide1-cd0<enter><wait10s>",
                "<leftCtrlOn><leftAltOn>3<leftAltOff><leftCtrlOff>",
                "<enter><wait5m>",
                "",




                "<wait20m>"
            ],
            "shutdown_command": "{{ user `shutdown_command` }}"
        }
    ]
}
